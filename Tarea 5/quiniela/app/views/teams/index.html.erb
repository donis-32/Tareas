<p style="color: green"><%= notice %></p>

<h1>Teams</h1>
<table border=1>
  <tr>
    <th>Nombre</th>
    <th>Acciones</th>
  </tr>
  <% @teams.each do |team| %>
    <tr>
        <td><%= team.name %></td>
        <td><%= link_to "Show this team", team %></td>
    </tr>
  <% end %>
</table>
</br>
<%= link_to "New team", new_team_path %>
